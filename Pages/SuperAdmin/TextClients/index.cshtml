<script>
    document.addEventListener('DOMContentLoaded', function () {
        const useButtons = document.querySelectorAll('.item button:not([disabled])');

        useButtons.forEach(btn => {
            btn.addEventListener('click', function (e) {
                e.preventDefault();

                const item = btn.closest('.item');
                const serialValue = item.querySelector('span').textContent.trim();

                const serialKeyTemplate = `
          <div class="serial-key d-flex flex-column gap-1">
              <span class="fs-14 color-000 fw-700">Serial No. Key</span>
              <div class="input-group">
                  <input type="text" name="serial-key[]" class="form-control px-3 color-666" value="${serialValue}" disabled>
              </div>
          </div>
        `;

                // Determine which container to use based on the viewport width
                const isMobile = window.matchMedia("(max-width: 576px)").matches;
                const container = isMobile
                    ? document.getElementById('serial-key-container-mobile')
                    : document.getElementById('serial-key-container');

                container.insertAdjacentHTML('beforeend', serialKeyTemplate);

                btn.disabled = true;
                btn.textContent = 'Used';
            });
        });
    });
</script>
