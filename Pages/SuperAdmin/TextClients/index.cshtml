<script>
    document.addEventListener("DOMContentLoaded", function () {
        const sortLinks = document.querySelectorAll("th a");

        sortLinks.forEach(link => {
            link.addEventListener("click", function (e) {
                e.preventDefault();

                const currentSort = link.getAttribute("data-sort");

                let nextSort;
                switch (currentSort) {
                    case "default":
                        nextSort = "ascending";
                        break;
                    case "ascending":
                        nextSort = "descending";
                        break;
                    case "descending":
                    default:
                        nextSort = "default";
                }

                link.setAttribute("data-sort", nextSort);

                const sortImg = link.querySelector("img");
                if (sortImg) {
                    if (nextSort === "ascending") {
                        sortImg.src = "/SuperAdmin/images/icons/sort-up.svg";
                        sortImg.alt = "sort-up";
                    } else if (nextSort === "descending") {
                        sortImg.src = "/SuperAdmin/images/icons/sort-down.svg";
                        sortImg.alt = "sort-down";
                    } else {
                        sortImg.src = "/SuperAdmin/images/icons/sort-default.svg";
                        sortImg.alt = "sort-default";
                    }
                }
            });
        });
    }); 
</script>

<script>
    document.addEventListener("DOMContentLoaded", function () {
        const mainView = document.getElementById("mainViewSection");
        const editView = document.getElementById("editSection");
        const offCanvas = document.getElementById("accountOffCanvasDropDown");
        const editViewMobileModal = new bootstrap.Modal(document.getElementById("editViewMobileModal"));
        let selectedRow = null;

        // Event delegation for desktop edit buttons
        document.addEventListener("click", function (event) {
            if (event.target.closest(".dropdown-item")?.id === "edit") {
                event.preventDefault();
                selectedRow = event.target.closest("tr");
                if (selectedRow) {
                    const userData = extractRowData(selectedRow);
                    // populateEditSection(userData);
                    swapView(editView);
                }
            }

            // Handle "Go Back" button
            if (event.target.closest("#go-back")) {
                event.preventDefault();
                swapView(mainView);
            }

            // Open offcanvas and store row reference for mobile
            if (event.target.closest(".more-icon-btn")) {
                selectedRow = event.target.closest("tr"); // Store row reference
            }

            // Mobile Edit Button inside the offcanvas
            if (event.target.closest("#editViewMobile")) {
                event.preventDefault();
                if (selectedRow) {
                    const userData = extractRowData(selectedRow);
                    // populateEditSection(userData);
                    editViewMobileModal.show(); // Show modal for mobile instead of swapping view

                    // Hide offcanvas manually
                    let offcanvasInstance = bootstrap.Offcanvas.getInstance(offCanvas);
                    if (offcanvasInstance) {
                        offcanvasInstance.hide();
                    }
                }
            }
            // Close mobile modal and go back to main view
            if (event.target.closest("[data-bs-dismiss='modal']")) {
                swapView(mainView);
            }
        });

        function extractRowData(row) {
            return {
                name: row.cells[1].textContent.trim(),
                email: row.cells[2].textContent.trim(),
                subscription: row.cells[3].textContent.trim(),
                startDate: row.cells[4].textContent.trim(),
                endDate: row.cells[5].textContent.trim(),
            };
        }

        function populateEditSection(data) {
            document.getElementById("editName").value = data.name;
            document.getElementById("editEmail").value = data.email;
            document.getElementById("editSubscription").value = data.subscription;
            document.getElementById("editStartDate").value = data.startDate;
            document.getElementById("editEndDate").value = data.endDate;
        }

        function swapView(view) {
            mainView.classList.toggle("d-none", view === editView);
            editView.classList.toggle("d-none", view === mainView);

            viewParent.innerHTML = "";
            viewParent.appendChild(view);
        }
    });
</script>

<script>
    document.addEventListener("DOMContentLoaded", function () {
        function toggleActivationStatus(checkbox, targetElement) {
            if (checkbox.checked) {
                targetElement.classList.remove("d-none");
                targetElement.classList.add("d-flex");
            } else {
                targetElement.classList.remove("d-flex");
                targetElement.classList.add("d-none");
            }
        }

        // Desktop Elements
        const statusCheckbox = document.getElementById("statusActivated");
        const activatedBySection = document.getElementById("activatedBy");

        if (statusCheckbox && activatedBySection) {
            statusCheckbox.addEventListener("change", function () {
                toggleActivationStatus(this, activatedBySection);
            });
        }

        // Mobile Elements
        const statusCheckboxMobile = document.getElementById("statusActivatedMobile");
        const activatedBySectionMobile = document.getElementById("activatedByMobile");

        if (statusCheckboxMobile && activatedBySectionMobile) {
            statusCheckboxMobile.addEventListener("change", function () {
                toggleActivationStatus(this, activatedBySectionMobile);
            });
        }
    });

</script>


